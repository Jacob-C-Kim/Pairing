{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.campusgroups.com https://campusgroups.com https://*.rit.edu;"
        }
      ]
    }
  ]
}
